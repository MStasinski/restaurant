
<div class="row">
  {{#each sortedModel as |insalate|}}
    <div class="col-md-12">
      <div class="panel panel-default library-item">
       
        <div class="panel-heading">
          <h3 class="panel-title">ID: {{insalate.rank}}&nbsp;&nbsp;&nbsp;&nbsp; {{insalate.name}}</h3>
        </div>

        <div class="panel-body">
          <h3 class="panel-title">Cena: {{insalate.price}}</h3>
        </div>
       
        <div class="panel-body">
          <p>opis: {{insalate.desc}}</p>
        </div>
        
        <div class="panel-footer text-right">
          {{#link-to 'insalates.edit' insalate.id class='btn btn-success btn-xs'}}Edytuj{{/link-to}}
          <button class="btn btn-danger btn-xs" {{action 'deleteLibrary' insalate}}>Usu≈Ñ</button>
        </div>
      </div>
    </div>
  {{/each}}
</div>
